	"item": [
		{
			"name": "get_Request",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"API 6pm Class Status code\",function(){\r",
							"    pm.response.to.be.status(200);\r",
							"})\r",
							"pm.test(\"validate status code\",()=>{\r",
							"  pm.expect(pm.response.code).to.oneOf([200,201,404])\r",
							"})\r",
							"pm.test(\"validate the header response\",()=>{\r",
							"    pm.response.to.have.header(\"content-type\")\r",
							"})\r",
							"pm.test(\"validate the header value\",()=>{\r",
							"    pm.expect(pm.response.headers.get('Transfer-Encoding')).to.eql('chunked')\r",
							"})\r",
							"pm.test(\"validate the cookies\",()=>{\r",
							"    pm.expect(pm.cookies.has('APITesting')).to.be.false;\r",
							"})\r",
							"pm.test(\"validate the response time\",function(){\r",
							"    pm.expect(pm.response.responseTime).to.be.above(50);\r",
							"})\r",
							"// const, let, var\r",
							"const responseVariable=pm.response.json();\r",
							"pm.test(\"validate the response body\",()=>{\r",
							"    pm.expect(responseVariable).to.be.an(\"object\");    \r",
							"    pm.expect(responseVariable.total).to.be.a(\"number\");\r",
							"})\r",
							"pm.test(\"validate  the response body values\",()=>{\r",
							"pm.expect(responseVariable.per_page).to.eql(pm.variables.get(\"Actual_perpage_value\"));\r",
							"})\r",
							"pm.test(\"validate the array\",()=>{\r",
							"    pm.expect(responseVariable.data[1].last_name).to.eql(\"Weaver\");\r",
							"})\r",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.variables.set(\"Actual_perpage_value\",6);"
						],
						"type": "text/javascript"
					}
				}
