				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.globals.set(\"vadapalani_new_variable\",\"https://reqres.in\");\r",
							"pm.collectionVariables.set(\"Collection_variable_vadapalani\",\"api/users\");\r",
							"pm.variables.set(\"value\",\"1\")\r",
							"pm.variables.set(\"validation_message\",\"To keep ReqRes free, contributions towards server costs are appreciated!\")"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"\r",
							"var responsevariableName =pm.response.json();\r",
							"\r",
							"pm.test(\"validate the status code\",function(){\r",
							"    pm.response.to.have.status(200);\r",
							"})\r",
							"pm.test(\"description\",function(){\r",
							"    pm.response.to.have.status(204)\r",
							"})\r",
							" pm.test(\"validate the status codes\",()=>{\r",
							"     pm.expect(pm.response.code).to.be.oneOf([200,201])\r",
							" })\r",
							"\r",
							" pm.test(\"validate the headers\",()=>{\r",
							"    pm.response.to.have.header('Content-Length');\r",
							"});\r",
							"\r",
							"pm.test(\"validate the content length value\",()=>{\r",
							"pm.expect(pm.response.headers.get('Content-Length')).to.eql(400)\r",
							"});\r",
							"\r",
							"pm.test(\"cookies validation\",function(){\r",
							"    pm.response.to.have.cookies('testing');\r",
							"})\r",
							"pm.test(\"cookies testing value validation\",function(){\r",
							"    pm.expect(pm.response.cookies.get('testing')).to.eql(\"API-Testing\")\r",
							"})\r",
							"pm.test(\"validate the response time\",function(){\r",
							"    pm.expect(pm.response.responseTime).to.be.above(100)\r",
							"})\r",
							"\r",
							"\r",
							"\r",
							"pm.globals.set(\"values_from_response\",responsevariableName.total)\r",
							"\r",
							"pm.test(\"validate the responseBody content\",function(){\r",
							"   \r",
							"});\r",
							"\r",
							"pm.test(\"validate the responseBody value\",function(){\r",
							"    pm.expect(responsevariableName.data).to.be.an(\"array\");\r",
							"    pm.expect(responsevariableName.total_pages).to.be.a(\"number\")\r",
							"    pm.expect(responsevariableName.data[1].first_name).to.eql(\"Janet\");\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"testing  body\",function() {\r",
							"\r",
							"     pm.expect(responsevariableName.per_page).to.eql(6);\r",
							"    pm.expect(responsevariableName.data[3].last_name).to.eql(\"Holt\");\r",
							"    pm.expect(responsevariableName.support.url).to.eql(\"https://reqres.in/#support-heading\");\r",
							"})\r",
							"\r",
							"pm.test(\"validation message\",()=>{\r",
							"    pm.expect(responsevariableName.support.text).to.eql(\"To keep ReqRes free, contributions towards server costs are appreciated!\");\r",
							"})\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							""
